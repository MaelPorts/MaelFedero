{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/client/index.html"
    },
    {
      "source": "/projects",
      "destination": "/client/pages/projects.html"
    },
    {
      "source": "/contact",
      "destination": "/client/pages/contact.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ]
}
